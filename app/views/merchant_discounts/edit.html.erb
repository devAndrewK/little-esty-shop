<%= render partial: "shared/merchant_header", object: @discount.merchant, :as => :merchant %>
<h1>Edit Discount</h1>
<%= form_with model: @discount, local: true do |f| %>
  <%= f.label :percentage %>
  <%= f.number_field :percentage %>

  <%= f.label :quantity %>
  <%= f.number_field :quantity %>

  <%= f.submit %>
<% end %>